{
  "name": "verse",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {},
  "aven": {
    "env": "web",
    "envOptions": {
      "mainServer": "MainServer.js",
      "mainClient": "MainClient.js",
      "deployEnv": "Script",
      "deployScript": "deploy.sh",
      "knexFile": {
        "development": {
          "client": "sqlite3",
          "useNullAsDefault": true,
          "migrations": {
            "directory": "src/sync/aven-cloud-sql/migrations"
          },
          "connection": {
            "filename": "cloud.sqlite"
          }
        }
      }
    },
    "srcDependencies": [
      "aven-web",
      "aven-admin",
      "aven-cloud",
      "aven-cloud-server",
      "aven-cloud-browser",
      "aven-cloud-sql",
      "ono-cloud",
      "components",
      "skynet"
    ],
    "moduleDependencies": [
      "stripe",
      "sqlite3",
      "fbjs",
      "ws",
      "rxjs-compat",
      "ethernet-ip",
      "@nozbe/with-observables"
    ]
  }
}
