{
  "name": "skynet",
  "aven": {
    "env": "web",
    "publicAssetDir": "public",
    "envOptions": {
      "deployEnv": "Heroku",
      "herokuAppName": "ono-skynet",
      "mainServer": "MainServer.js",
      "mainClient": "MainClient.js",
      "knexFile": {
        "development": {
          "client": "sqlite3",
          "useNullAsDefault": true,
          "migrations": {
            "directory": "src/sync/aven-cloud-sql/migrations"
          },
          "connection": {
            "filename": "cloud.sqlite"
          }
        }
      }
    },
    "srcDependencies": [
      "aven-web",
      "ono-cloud",
      "aven-cloud-auth",
      "aven-cloud-auth-email",
      "aven-cloud-auth-sms",
      "aven-cloud-auth-root",
      "aven-cloud-utils",
      "aven-email-agent-sendgrid",
      "aven-sms-agent-twilio",
      "aven-admin",
      "aven-cloud",
      "cloud-server",
      "aven-cloud-sql"
    ],
    "moduleDependencies": [
      "sqlite3",
      "@react-navigation/core",
      "airtable",
      "wget-improved",
      "square-connect",
      "fs-extra"
    ]
  }
}
