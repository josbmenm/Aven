{
  "name": "ono-cloud",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "airtable": "^0.5.6",
    "body-parser": "^1.18.3",
    "express-enforces-ssl": "^1.1.0",
    "helmet": "^3.13.0",
    "json-stable-stringify": "^1.0.1",
    "pg": "^7.4.3",
    "square-connect": "^2.20180918.0",
    "uuid": "^3.3.2",
    "wget-improved": "^3.0.1",
    "yes-https": "^0.0.4"
  },
  "globe": {
    "env": "web",
    "envOptions": {
      "deployEnv": "GoogleAppEngine",
      "mainServer": "MainServer.js",
      "mainClient": "MainClient.js"
    },
    "globeDependencies": [
      "aven-web",
      "aven-data-server",
      "aven-data-source-postgres"
    ],
    "publicBuildConfigVars": [
      "BUILD_ENV"
    ],
    "secretBuildConfigVars": [
      "SQUARE_TOKEN",
      "SQUARE_LOCATION_ID",
      "AIRTABLE_API_KEY",
      "AIRTABLE_BASE_ID",
      "SQL_INSTANCE_CONNECTION_NAME",
      "SQL_HOST",
      "SQL_DATABASE",
      "SQL_USER",
      "SQL_PASSWORD"
    ]
  }
}
