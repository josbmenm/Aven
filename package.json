{
  "name": "onofoodco",
  "scripts": {
    "start": "./node_modules/@aven-cloud/aven/aven-tools/cli.js start",
    "build": "./node_modules/@aven-cloud/aven/aven-tools/cli.js build",
    "deploy": "./node_modules/@aven-cloud/aven/aven-tools/cli.js deploy",
    "start-skynet": "PORT=8840 ./node_modules/@aven-cloud/aven/aven-tools/cli.js start skynet",
    "deploy-skynet": "./node_modules/@aven-cloud/aven/aven-tools/cli.js deploy skynet",
    "backup-skynet": "node script/BackupSkynet.js",
    "start-status": "./node_modules/@aven-cloud/aven/aven-tools/cli.js start ono-kitchen-status",
    "start-verse": "PORT=8830 ./node_modules/@aven-cloud/aven/aven-tools/cli.js start verse",
    "start-blitz": "./node_modules/@aven-cloud/aven/aven-tools/cli.js start blitz",
    "start-blitz-app": "cd env-blitz-ios && react-native run-ios --simulator=\"iPad Pro (12.9-inch) (3rd generation)\"",
    "open-xcode": "open env-kiosk/ios/kiosk.xcworkspace",
    "start-portal": "./node_modules/@aven-cloud/aven/aven-tools/cli.js start portal",
    "clean": "./node_modules/@aven-cloud/aven/aven-tools/cli.js clean",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.1.0"
  },
  "dependencies": {
    "@aven-cloud/aven": "https://github.com/AvenCloud/Aven.git#master",
    "@nozbe/with-observables": "^1.0.1",
    "@react-navigation/core": "^3.0.0-alpha.10",
    "@react-navigation/native": "^3.0.0-alpha.5",
    "airtable": "^0.5.7",
    "body-parser": "^1.18.3",
    "crypto-js": "^3.1.9-1",
    "cuid": "^2.1.6",
    "ethernet-ip": "^1.2.2",
    "fbjs": "^1.0.0",
    "fs-extra": "^7.0.0",
    "helmet": "^3.13.0",
    "json-stable-stringify": "^1.0.1",
    "knex": "^0.16.3",
    "mjml": "^4.4.0-beta.1",
    "mjml-react": "^1.0.52",
    "path": "^0.12.7",
    "pg-format": "^1.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-native": "^0.58.5",
    "react-native-fast-image": "^5.1.2",
    "react-navigation-hooks": "^1.0.0-alpha.2",
    "react-navigation-stack": "1.0.0-alpha.22",
    "react-navigation-transitioner": "^1.0.0-alpha.3",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "square-connect": "^2.20180918.0",
    "stripe": "^6.19.0",
    "wget-improved": "^3.0.1",
    "ws": "^6.0.0",
    "yes-https": "^0.0.4"
  },
  "aven": {
    "extendsSrcModule": "@aven-cloud/aven"
  },
  "jest": {
    "testRegex": "/__tests__/[^/]+-test\\.(js|tsx)?$",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example/",
      "<rootDir>/env-blitz-ios/",
      "<rootDir>/node_modules/"
    ],
    "globals": {
      "__DEV__": true
    }
  }
}
