{
  "name": "aven",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "pg": "cd src/aven/cloud-postgres; docker-compose up",
    "dev": "node ./node_modules/@react-native-community/cli/build/bin.js start --reset-cache",
    "wing:dev": "concurrently \"yarn wing:dev-server\" \"yarn dev\"",
    "wing:dev-server": "cross-env CLIENT_SRC=aven/wing-web/WingWebClient.js BABEL_ENV=web-development NODE_ENV=development yarn babel-node src/aven/wing-web/WingWebServer.js",
    "wing:xcode": "open wing-mobile/ios/wing.xcworkspace",
    "wing:run-ios": "yarn react-native run-ios --project-path wing-mobile/ios",
    "wing:pods": "cd wing-mobile/ios && pod install && cd -",
    "wing:build": "rimraf dist/wing && yarn wing:create-bundles",
    "wing:start": "cross-env NODE_ENV=production node dist/wing/server/aven/wing-web/WingWebServer.js",
    "wing:create-bundles": "concurrently \"yarn wing:bundle-client\" \"yarn wing:bundle-server\" \"yarn wing:create-asset-manifest\"",
    "wing:bundle-server": "cross-env NODE_ENV=web-production yarn babel -d dist/wing/server src",
    "wing:bundle-client": "mkdirp dist/wing/client dist/wing/public && react-native bundle --entry-file src/aven/wing-web/WingWebClient.js --dev false --platform web --reset-cache --bundle-output dist/wing/public/main.js --assets-dest dist/wing/client",
    "wing:create-asset-manifest": "node scripts/bundleAssets.js wing aven/wing-web/WingWebClient.js",
    "pilot:xcode": "open src/aven/pilot-mobile/ios/pilot.xcworkspace",
    "pilot:run-ios": "yarn react-native run-ios --project-path src/aven/pilot-mobile/ios",
    "pilot:pods": "cd src/aven/pilot-mobile/ios && pod install && cd -",
    "pilot:dev": "concurrently \"yarn pilot:dev-server\" \"yarn dev\"",
    "pilot:dev-server": "cross-env CLIENT_SRC=aven/pilot-web/PilotWebClient.js BABEL_ENV=web-development NODE_ENV=development yarn babel-node src/aven/pilot-web/PilotWebServer.js",
    "pilot:build": "rimraf dist/pilot && yarn pilot:create-bundles",
    "pilot:start": "cross-env NODE_ENV=production node dist/pilot/server/aven/pilot-web/PilotWebServer.js",
    "pilot:create-bundles": "concurrently \"yarn pilot:bundle-client\" \"yarn pilot:bundle-server\" \"yarn pilot:create-asset-manifest\"",
    "pilot:bundle-server": "cross-env NODE_ENV=web-production yarn babel -d dist/pilot/server src",
    "pilot:bundle-client": "mkdirp dist/pilot/client dist/pilot/public && react-native bundle --entry-file src/aven/pilot-web/PilotWebClient.js --dev false --platform web --reset-cache --bundle-output dist/pilot/public/main.js --assets-dest dist/pilot/client",
    "pilot:create-asset-manifest": "node scripts/bundleAssets.js pilot aven/pilot-web/PilotWebClient.js",
    "legal-experimental": "cross-env BABEL_ENV=web-development NODE_ENV=development yarn babel-node src/aven/legal-experimental/test.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.8.4",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/cli": "git://github.com/AvenCloud/react-native-cli.git#master",
