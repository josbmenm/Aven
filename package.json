{
  "name": "@aven-cloud/aven",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./aven-tools/cli.js start",
    "build": "node ./aven-tools/cli.js build",
    "deploy": "node ./aven-tools/cli.js deploy",
    "publish": "node ./aven-tools/cli.js publish",
    "lint": "eslint .",
    "tlint": "tslint --project tsconfig.json",
    "format": "eslint . --fix",
    "clean": "node ./aven-tools/cli.js clean",
    "import-vendor": "node ./aven-tools/importVendorCode.js",
    "test": "jest",
    "pg": "cd cloud-postgres; docker-compose up",
    "test-sql": "cd cloud-postgres; ./test.sh startPostgresStorageSource",
    "carriage": "node carriage/carriage.js",
    "mobile": "yarn workspace @aven-cloud/something-mobile"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "babel-jest": "^24.8.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^5.16.0",
    "eslint-config-satya164": "^2.4.1",
    "eslint-plugin-react-native-globals": "^0.1.2",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.0",
    "pn": "^1.1.0",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.6.3",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.3.0",
    "@expo/vector-icons": "^8.0.0",
    "@sendgrid/mail": "^6.3.1",
    "@types/jest": "^23.3.10",
    "@types/knex": "^0.15.1",
    "ajv": "^6.6.2",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "3.0.6",
    "body-parser": "^1.18.3",
    "create-react-context": "^0.2.3",
    "crypto-js": "^3.1.9-1",
    "cuid": "^2.1.6",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^3.0.1",
    "isbinaryfile": "^4.0.0",
    "jest-expo": "^31.0.0",
    "js-yaml": "^3.12.0",
    "json-stable-stringify": "^1.0.1",
    "knex": "^0.16.3",
    "konva": "^3.2.1",
    "mime": "^2.4.4",
    "minimist": "^1.2.0",
    "node-fetch": "^2.3.0",
    "path": "^0.12.7",
    "path-to-regexp": "^1.7.0",
    "pg": "^7.5.0",
    "pg-format": "^1.0.4",
    "prism-react-renderer": "^0.1.5",
    "query-string": "^6.2.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-art": "16.9.0",
    "react-dom": "16.9.0",
    "react-is": "^16.5.2",
    "react-konva": "^16.8.4",
    "react-lifecycles-compat": "^3.0.4",
    "react-native-web": "^0.10.0",
    "reconnecting-websocket": "^4.0.0-rc5",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "sane": "^3.0.0",
    "semver": "^5.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "twilio": "^3.24.0",
    "websocket": "^1.0.28",
    "ws": "^6.0.0",
    "xstream": "^11.10.0",
    "yes-https": "^0.0.4"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "/__tests__/[^/]+-test\\.(js|tsx)?$",
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "cloud-postgres/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example/",
      "<rootDir>/aven-tools/proto/"
    ],
    "globals": {
      "__DEV__": true,
      "ts-jest": {
        "tsConfig": "./tsconfig.test.json",
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },
  "optionalDependencies": {
    "win-node-env": "^0.4.0"
  }
}
