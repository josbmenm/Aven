{
  "version": "0.0.0",
  "scripts": {
    "wing:dev": "concurrently \"npm run wing:dev-server\" \"npm run wing:dev-mobile\"",
    "wing:dev-server": "cross-env BABEL_ENV=web-development NODE_ENV=development yarn babel-node src/aven/wing-web/WingWebServer.js",
    "wing:dev-mobile": "react-native start",
    "wing:xcode": "open wing-mobile/ios/wing.xcworkspace",
    "wing:run-ios": "yarn react-native run-ios --project-path wing-mobile/ios",
    "wing:pods": "cd wing-mobile/ios && pod install && cd -",
    "wing:build": "rimraf dist && npm run wing:create-bundles",
    "wing:start": "cross-env NODE_ENV=production node dist/wing/server/aven/wing-web/WingWebServer.js",
    "wing:create-bundles": "concurrently \"npm run wing:bundle-client\" \"npm run wing:bundle-server\" \"npm run wing:create-asset-manifest\"",
    "wing:bundle-server": "cross-env NODE_ENV=web-production yarn babel -d dist/wing/server src",
    "wing:bundle-client": "mkdir -p dist/wing/client && react-native bundle --entry-file src/aven/wing-web/WingWebClient.js --dev false --platform web --reset-cache --bundle-output dist/wing/client/main.js --assets-dest dist/wing/client",
    "wing:create-asset-manifest": "node scripts/bundleAssets.js wing aven/wing-web/WingWebClient.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.8.4",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/cli": "git://github.com/AvenCloud/react-native-cli.git#master",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "^8.0.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "react-native": "^0.61.5",
    "react-test-renderer": "16.9.0",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@expo/spawn-async": "^1.3.0",
    "@expo/vector-icons": "^8.0.0",
    "@sendgrid/mail": "^6.3.1",
    "@types/jest": "^23.3.10",
    "ajv": "^6.6.2",
    "bcrypt": "3.0.6",
    "bcryptjs": "^2.4.3",
