{
  "name": "onofoodco",
  "scripts": {
    "start": "node node_modules/@aven-cloud/aven/aven-tools/cli.js start",
    "build": "node node_modules/@aven-cloud/aven/aven-tools/cli.js build",
    "deploy": "node node_modules/@aven-cloud/aven/aven-tools/cli.js deploy",
    "start-skynet": "PORT=8840 node node_modules/@aven-cloud/aven/aven-tools/cli.js start skynet",
    "deploy-skynet": "node node_modules/@aven-cloud/aven/aven-tools/cli.js deploy skynet",
    "deploy-bender": "./Bender/deploy-bender",
    "backup-skynet": "node script/BackupSkynet.js",
    "start-status": "node node_modules/@aven-cloud/aven/aven-tools/cli.js start ono-kitchen-status",
    "start-verse": "PORT=8830 node node_modules/@aven-cloud/aven/aven-tools/cli.js start verse",
    "test-verse": "VERSE_TEST=true PORT=8830 node node_modules/@aven-cloud/aven/aven-tools/cli.js start verse",
    "start-blitz": "node node_modules/@aven-cloud/aven/aven-tools/cli.js start blitz",
    "start-blitz-app": "cd env-blitz-ios && react-native run-ios --simulator=\"iPad Pro (12.9-inch) (3rd generation)\"",
    "open-xcode": "open env-downpour/ios/downpour.xcworkspace",
    "start-portal": "node node_modules/@aven-cloud/aven/aven-tools/cli.js start portal",
    "answer": "node node_modules/@aven-cloud/aven/aven-tools/cli.js start answer",
    "clean": "node node_modules/@aven-cloud/aven/aven-tools/cli.js clean",
    "process": "node process/goProcess.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "babel-jest": "^24.1.0",
    "plist": "^3.0.1"
  },
  "dependencies": {
    "@aven-cloud/aven": "https://github.com/AvenCloud/Aven.git#master",
    "@google-cloud/storage": "^3.0.2",
    "@nozbe/with-observables": "^1.0.1",
    "airtable": "^0.5.7",
    "aws-sdk": "^2.572.0",
    "body-parser": "^1.18.3",
    "crypto-js": "^3.1.9-1",
    "cuid": "^2.1.6",
    "ethernet-ip": "^1.2.2",
    "fbjs": "^1.0.0",
    "fs-extra": "^7.0.0",
    "geoip-lite": "^1.3.7",
    "helmet": "^3.13.0",
    "json-stable-stringify": "^1.0.1",
    "mjml": "^4.4.0-beta.1",
    "mjml-react": "^1.0.52",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "pg-format": "^1.0.4",
    "react": "^16.9.0",
    "react-calendar": "^2.19.2",
    "react-dom": "^16.9.0",
    "react-native-fast-image": "^5.1.2",
    "react-native-masked-text": "^1.12.3",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "square-connect": "^2.20180918.0",
    "stripe": "^6.19.0",
    "victory": "^34.0.1",
    "wget-improved": "^3.0.1",
    "ws": "^6.0.0",
    "xstream": "^11.11.0",
    "yes-https": "^0.0.4"
  },
  "aven": {
    "extendsSrcModule": "@aven-cloud/aven"
  },
  "jest": {
    "testRegex": "/__tests__/[^/]+-test\\.(js|tsx)?$",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example/",
      "<rootDir>/env-kiosk/",
      "<rootDir>/env-blitz-ios/",
      "<rootDir>/node_modules/"
    ],
    "globals": {
      "__DEV__": true
    }
  }
}
